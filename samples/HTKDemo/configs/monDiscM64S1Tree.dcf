Trace_tool_calls: y

<ENDsys_setup>:

<BEGINtool_steps>

HCopy: n
HList: n
HQuant: y
HLEd: n
HInit: y
HRest: y
HERest: y
HSmooth: n
HVite: y

<ENDtool_steps>:

<ENDtest_config_file>:


